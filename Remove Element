class Solution {
public:
    int removeElement(vector<int>& arr, int val) {
        int i;
        int size = arr.size();
        int index = 0;

        for(i=0;i<size;i++)
        {
            if(arr[i]!=val)
            {
                arr[index] = arr[i];
                index++;
            }
        }
        return index;
    }
};
